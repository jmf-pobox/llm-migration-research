  "operator_mappings": {
    "+": {
      "name": "Addition",
      "precedence": 1,
      "associativity": "left",
      "latex_symbol": "+"
    },
    "-": {
      "name": "Subtraction",
      "precedence": 1,
      "associativity": "left",
      "latex_symbol": "-"
    },
    "*": {
      "name": "Multiplication",
      "precedence": 2,
      "associativity": "left",
      "latex_symbol": "\\times"
    },
    "/": {
      "name": "Division",
      "precedence": 2,
      "associativity": "left",
      "latex_symbol": "\\div"
    }
  },
  "output_format_rules": {
    "math_mode_delimiters": "$...$",
    "spacing_around_operators": "single_space",
